{
    "name": "bhittani/path",
    "license": "MIT",
    "description": "Utilities for working with paths.",
    "keywords": ["path", "utility"],
    "homepage": "https://github.com/kamalkhan/path",
    "authors": [
        {
            "role": "developer",
            "name": "Kamal Khan",
            "email": "shout@bhittani.com",
            "homepage": "http://bhittani.com"
        }
    ],
    "support": {
        "source": "https://github.com/kamalkhan/path",
        "issues": "https://github.com/kamalkhan/path/issues"
    },
    "autoload": {
        "psr-4": {
            "Bhittani\\Path\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bhittani\\Path\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "platform": { "php": "5.6" }
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "<8"
    },
    "scripts": {
        "test": "phpunit --colors=always"
    },
    "repositories": [
        { "packagist": false },
        { "type": "composer", "url": "https://packagist.org" }
    ]
}
